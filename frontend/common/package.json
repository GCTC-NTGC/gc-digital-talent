{
  "name": "gc-digital-talent-common",
  "version": "1.0.11",
  "license": "MIT",
  "description": "Shared components and utilities for GC Digital Talent apps",
  "scripts": {
    "tsc": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.ts,.jsx,.tsx",
    "prettier": "prettier --write ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "codegen": "graphql-codegen",
    "h2-compile": "h2-compile",
    "h2-watch": "h2-watch",
    "h2-build": "h2-build",
    "intl-extract": "formatjs extract './src/**/*.{ts,tsx}' --ignore './**/*.d.ts' --out-file src/lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl-compile": "formatjs compile src/lang/fr.json --out-file src/lang/frCompiled.json",
    "check-intl-common": "node ./src/tooling/checkIntl.js --en src/lang/en.json --fr src/lang/fr.json --rm-orphaned --output-untranslated src/lang/untranslated.json --whitelist src/lang/whitelist.json",
    "check-intl-common-merge": "node src/tooling/checkIntl.js --en src/lang/en.json --fr src/lang/fr.json --rm-orphaned --output-untranslated src/lang/untranslated.json --whitelist src/lang/whitelist.json --merge-fr src/lang/newTranslations.json",
    "check-intl-admin": "node ./src/tooling/checkIntl.js --en ../admin/src/js/lang/en.json --fr ../admin/src/js/lang/fr.json --rm-orphaned --output-untranslated ../admin/src/js/lang/untranslated.json --whitelist ../admin/src/js/lang/whitelist.json",
    "check-intl-admin-merge": "node ./src/tooling/checkIntl.js --en ../admin/src/js/lang/en.json --fr ../admin/src/js/lang/fr.json --rm-orphaned --output-untranslated ../admin/src/js/lang/untranslated.json --whitelist ../admin/src/js/lang/whitelist.json --merge-fr ../admin/src/js/lang/newTranslations.json",
    "check-intl-talentsearch": "node ./src/tooling/checkIntl.js --en ../talentsearch/src/js/lang/en.json --fr ../talentsearch/src/js/lang/fr.json --rm-orphaned --output-untranslated ../talentsearch/src/js/lang/untranslated.json --whitelist ../talentsearch/src/js/lang/whitelist.json",
    "check-intl-talentsearch-merge": "node ./src/tooling/checkIntl.js --en ../talentsearch/src/js/lang/en.json --fr ../talentsearch/src/js/lang/fr.json --rm-orphaned --output-untranslated ../talentsearch/src/js/lang/untranslated.json --whitelist ../talentsearch/src/js/lang/whitelist.json --merge-fr ../talentsearch/src/js/lang/newTranslations.json",
    "check-intl-indigenousapprenticeship": "node ./src/tooling/checkIntl.js --en ../indigenousapprenticeship/src/js/lang/en.json --fr ../indigenousapprenticeship/src/js/lang/fr.json --rm-orphaned --output-untranslated ../indigenousapprenticeship/src/js/lang/untranslated.json --whitelist ../indigenousapprenticeship/src/js/lang/whitelist.json",
    "check-intl-indigenousapprenticeship-merge": "node ./src/tooling/checkIntl.js --en ../indigenousapprenticeship/src/js/lang/en.json --fr ../indigenousapprenticeship/src/js/lang/fr.json --rm-orphaned --output-untranslated ../indigenousapprenticeship/src/js/lang/untranslated.json --whitelist ../indigenousapprenticeship/src/js/lang/whitelist.json --merge-fr ../indigenousapprenticeship/src/js/lang/newTranslations.json",
    "check-integrity": "node ./src/tooling/checkIntegrity.js --lock-file ../package-lock.json --allow common admin talentsearch indigenousapprenticeship"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GCTC-NTGC/gc-digital-talent.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/GCTC-NTGC/gc-digital-talent/issues"
  },
  "homepage": "https://github.com/GCTC-NTGC/gc-digital-talent#readme",
  "engines": {
    "node": "14.18.1",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.32.0"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@reach/dialog": "^0.17.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@urql/exchange-auth": "^0.1.7",
    "graphql": "^16.4.0",
    "history": "^5.3.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.9.1",
    "react-hook-form": "^7.32.0",
    "react-intl": "^6.0.4",
    "react-remove-scroll": "^2.5.4",
    "react-select": "^5.3.1",
    "react-toastify": "^9.0.4",
    "universal-router": "^9.1.0",
    "urql": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@faker-js/faker": "^7.2.0",
    "@formatjs/cli": "^4.8.4",
    "@formatjs/ts-transformer": "^3.9.8",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-urql": "^3.5.12",
    "@hydrogen-design-system/hydrogen.css": "^1.1.19",
    "@storybook/addon-actions": "*",
    "@storybook/addon-essentials": "*",
    "@storybook/addon-links": "*",
    "@storybook/builder-webpack5": "*",
    "@storybook/manager-webpack5": "*",
    "@storybook/react": "^6.4.16",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.42",
    "@types/path-browserify": "^1.0.0",
    "@types/react": "^17.0.43",
    "@types/react-select": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^3.1.5",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.6.2",
    "storybook-addon-intl": "^2.4.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "yargs": "^17.5.1"
  }
}
