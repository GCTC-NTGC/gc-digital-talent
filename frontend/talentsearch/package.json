    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-intl": "^5.25.0",
    "react-select": "^5.2.1",
    "react-toastify": "^8.2.0",
    "regenerator-runtime": "^0.13.9",
    "universal-router": "^9.1.0",
    "urql": "^2.2.0"
  }
