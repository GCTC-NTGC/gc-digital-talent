{
  "name": "admin",
  "version": "1.3.2",
  "description": "Client side service using webpack and ReactJS.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GCTC-NTGC/gc-digital-talent.git"
