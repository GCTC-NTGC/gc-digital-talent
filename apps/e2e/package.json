{
  "name": "@gc-digital-talent/e2e",
  "version": "0.0.1",
  "scripts": {
    "e2e": "pnpm run e2e:open",
    "e2e:open": "cypress open --e2e",
    "e2e:run": "pnpm run e2e:run:all",
    "e2e:run:all": "cypress run --browser=electron --spec \"cypress/e2e/**/*${TEST_FILTER:-}*.cy.ts\"",
    "e2e:run:inspect": "cypress run --browser=electron --spec \"cypress/e2e/**/*${TEST_FILTER:-language-selection}*.cy.ts\" --no-exit --headed",
    "lint": "eslint ./cypress --ext .js,.ts"
  },
  "dependencies": {
    "@gc-digital-talent/date-helpers": "workspace:*",
    "@gc-digital-talent/eslint-config": "workspace:*",
    "@gc-digital-talent/graphql": "workspace:*"
  },
  "devDependencies": {
    "@gc-digital-talent/eslint-config": "*",
    "@testing-library/cypress": "^10.0.2",
    "@types/node": "^20.14.6",
    "axe-core": "^4.9.1",
    "cy-verify-downloads": "^0.2.4",
    "cypress": "^13.12.0",
    "cypress-axe": "^1.5.0",
    "cypress-terminal-report": "^6.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "jwt-decode": "4.0.0",
    "prettier": "^3.3.2",
    "tsconfig": "workspace:*",
    "typescript": "5.4.5"
  }
}
