{
  "name": "@gc-digital-talent/e2e",
  "version": "0.0.1",
  "scripts": {
    "e2e": "pnpm e2e:open",
    "e2e:open": "cypress open --e2e",
    "e2e:run": "pnpm e2e:run:all",
    "e2e:run:all": "cypress run --browser=electron --spec \"cypress/e2e/**/*${TEST_FILTER:-}*.cy.ts\"",
    "e2e:run:inspect": "cypress run --browser=electron --spec \"cypress/e2e/**/*${TEST_FILTER:-language-selection}*.cy.ts\" --no-exit --headed"
  },
  "dependencies": {
    "@gc-digital-talent/date-helpers": "workspace:*",
    "@gc-digital-talent/graphql": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@types/node": "^20.11.23",
    "axe-core": "^4.8.4",
    "cy-verify-downloads": "^0.2.3",
    "cypress": "^13.6.6",
    "cypress-axe": "^1.5.0",
    "cypress-terminal-report": "^6.0.1",
    "dotenv": "^16.4.5",
    "eslint-plugin-cypress": "^2.15.1",
    "jwt-decode": "4.0.0",
    "prettier": "^3.2.5",
    "tsconfig": "workspace:*",
    "typescript": "4.9.5"
  }
}
