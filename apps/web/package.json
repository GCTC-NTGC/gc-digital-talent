    "@gc-digital-talent/toast": "workspace:*",
    "@gc-digital-talent/ui": "workspace:*",
    "@heroicons/react": "^2.1.3",
    "@microsoft/applicationinsights-web": "^3.1.1",
    "@tanstack/react-table": "^8.13.2",
    "dataloader": "^2.2.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^11.0.13",
    "graphql": "^16.8.1",
