{
  "name": "@gc-digital-talent/web",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "codegen": "graphql-codegen",
    "dev": "webpack --config webpack.dev.js",
    "intl-extract": "formatjs extract './src/**/*.{ts,tsx}' --ignore './**/*.d.ts' --out-file src/lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl-compile": "formatjs compile ./src/lang/fr.json --ast --out-file ./src/lang/frCompiled.json",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.ts,.jsx,.tsx",
    "prettier": "prettier --write ./src/js",
    "production": "npm run build",
    "storybook": "start-storybook -p 6006",
    "tsc": "tsc --project tsconfig.json --noEmit",
    "watch": "webpack watch --config webpack.dev.js"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@microsoft/applicationinsights-react-js": "^3.4.0",
    "@microsoft/applicationinsights-web": "^2.8.9",
    "@storybook/addon-viewport": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "dotenv": "^16.0.3",
    "framer-motion": "^6.5.1",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "~7.38.0",
    "react-intl": "^6.2.5",
    "react-router-dom": "^6.4.3",
    "react-toastify": "^9.1.1",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@formatjs/cli": "^5.1.3",
    "@formatjs/ts-transformer": "^3.11.5",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@hydrogen-css/hydrogen": "2.0.0-beta.33",
    "@storybook/builder-webpack5": "^6.5.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/jest-axe": "^3.5.5",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-helmet": "^6.1.6",
    "babel-plugin-formatjs": "^10.3.35",
    "eslint": "^8.30.0",
    "eslint-config-custom": "*",
    "hydrogen-webpack-plugin": "*",
    "jest": "^29.3.1",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "storybook-addon-intl": "^2.4.1",
    "storybook-helpers": "*",
    "ts-jest": "^29.0.3",
    "tsconfig": "*",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-config": "*",
    "webpack-merge": "^5.8.0"
  }
}
