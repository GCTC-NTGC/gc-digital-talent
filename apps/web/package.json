{
  "name": "@gc-digital-talent/web",
  "private": true,
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "description": "",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "webpack --config webpack.dev.js",
    "intl-extract": "formatjs extract './src/**/*.{ts,tsx}' --ignore './**/*.d.ts' --out-file src/lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl-compile": "formatjs compile ./src/lang/fr.json --ast --out-file ./src/lang/frCompiled.json && formatjs compile ./src/lang/crg.json --ast --out-file ./src/lang/crgCompiled.json && formatjs compile ./src/lang/crk.json --ast --out-file ./src/lang/crkCompiled.json && formatjs compile ./src/lang/ojw.json --ast --out-file ./src/lang/ojwCompiled.json && formatjs compile ./src/lang/mic.json --ast --out-file ./src/lang/micCompiled.json",
    "intl-compile-fr": "formatjs compile ./src/lang/fr.json --ast --out-file ./src/lang/frCompiled.json",
    "intl-compile-crg": "formatjs compile ./src/lang/crg.json --ast --out-file ./src/lang/crgCompiled.json",
    "intl-compile-crk": "formatjs compile ./src/lang/crk.json --ast --out-file ./src/lang/crkCompiled.json",
    "intl-compile-ojw": "formatjs compile ./src/lang/ojw.json --ast --out-file ./src/lang/ojwCompiled.json",
    "intl-compile-mic": "formatjs compile ./src/lang/mic.json --ast --out-file ./src/lang/micCompiled.json",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.ts,.jsx,.tsx",
    "prettier": "prettier --write ./src/js",
    "production": "pnpm run build",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "storybook:design-system": "SB_APP=design storybook dev -p 6006",
    "storybook:web": "SB_APP=web storybook dev -p 6006",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "tsc": "tsc --project tsconfig.json --noEmit",
    "watch": "webpack watch --config webpack.dev.js"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "@gc-digital-talent/app-insights": "workspace:*",
