    "eslint-config-custom": "*",
    "hydrogen-webpack-plugin": "*",
    "jest": "^29.5.0",
    "jest-axe": "^7.0.1",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "storybook-addon-intl": "^3.2.0",
    "storybook-helpers": "*",
    "ts-jest": "^29.1.0",
    "tsconfig": "*",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-config": "*",
    "webpack-merge": "^5.9.0"
  }
}
