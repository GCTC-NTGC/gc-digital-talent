    "@faker-js/faker": "^8.4.1",
    "@formatjs/cli": "^6.2.7",
    "@gc-digital-talent/eslint-config": "workspace:*",
    "@gc-digital-talent/fake-data": "workspace:*",
    "@gc-digital-talent/helpers": "workspace:*",
    "@gc-digital-talent/jest-helpers": "workspace:*",
    "@gc-digital-talent/jest-presets": "workspace:*",
    "@gc-digital-talent/storybook-helpers": "workspace:*",
    "@gc-digital-talent/webpack-config": "workspace:*",
    "@hydrogen-css/hydrogen": "2.0.5",
