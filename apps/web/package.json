    "babel-plugin-formatjs": "^10.5.13",
    "core-js": "^3.36.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "hydrogen-webpack-plugin": "workspace:*",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsconfig": "workspace:*",
    "typescript": "^5.4.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  }
