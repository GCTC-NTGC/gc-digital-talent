{
  "name": "@gc-digital-talent/web",
  "private": true,
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "description": "",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "codegen": "graphql-codegen",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "webpack --config webpack.dev.js",
    "intl-extract": "formatjs extract './src/**/*.{ts,tsx}' --ignore './**/*.d.ts' --out-file src/lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl-compile": "formatjs compile ./src/lang/fr.json --ast --out-file ./src/lang/frCompiled.json && formatjs compile ./src/lang/crg.json --ast --out-file ./src/lang/crgCompiled.json && formatjs compile ./src/lang/crk.json --ast --out-file ./src/lang/crkCompiled.json && formatjs compile ./src/lang/ojw.json --ast --out-file ./src/lang/ojwCompiled.json && formatjs compile ./src/lang/mic.json --ast --out-file ./src/lang/micCompiled.json",
    "intl-compile-fr": "formatjs compile ./src/lang/fr.json --ast --out-file ./src/lang/frCompiled.json",
    "intl-compile-crg": "formatjs compile ./src/lang/crg.json --ast --out-file ./src/lang/crgCompiled.json",
    "intl-compile-crk": "formatjs compile ./src/lang/crk.json --ast --out-file ./src/lang/crkCompiled.json",
    "intl-compile-ojw": "formatjs compile ./src/lang/ojw.json --ast --out-file ./src/lang/ojwCompiled.json",
    "intl-compile-mic": "formatjs compile ./src/lang/mic.json --ast --out-file ./src/lang/micCompiled.json",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.ts,.jsx,.tsx",
    "prettier": "prettier --write ./src/js",
    "production": "npm run build",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "storybook:design-system": "SB_APP=design storybook dev -p 6006",
    "storybook:web": "SB_APP=web storybook dev -p 6006",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "tsc": "tsc --project tsconfig.json --noEmit",
    "watch": "concurrently --kill-others-on-fail \"npm run codegen -- --watch\" \"webpack watch --config webpack.dev.js\""
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "@gc-digital-talent/auth": "*",
    "@gc-digital-talent/client": "*",
    "@gc-digital-talent/date-helpers": "*",
    "@gc-digital-talent/env": "*",
    "@gc-digital-talent/forms": "*",
    "@gc-digital-talent/graphql": "*",
    "@gc-digital-talent/helpers": "*",
    "@gc-digital-talent/i18n": "*",
    "@gc-digital-talent/logger": "*",
    "@gc-digital-talent/storage": "*",
    "@gc-digital-talent/theme": "*",
    "@gc-digital-talent/toast": "*",
    "@gc-digital-talent/ui": "*",
    "@heroicons/react": "^2.1.1",
    "@microsoft/applicationinsights-web": "^3.0.7",
    "@tanstack/react-table": "^8.11.3",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-intl": "^6.5.5",
    "react-router-dom": "^6.21.1",
    "react-to-print": "^2.14.15",
    "urql": "^4.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@faker-js/faker": "^8.3.1",
    "@formatjs/cli": "^6.2.4",
    "@gc-digital-talent/fake-data": "*",
    "@gc-digital-talent/helpers": "*",
    "@gc-digital-talent/jest-helpers": "*",
    "@gc-digital-talent/storybook-helpers": "*",
    "@gc-digital-talent/webpack-config": "*",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@hydrogen-css/hydrogen": "2.0.3",
    "@parcel/watcher": "^2.3.0",
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-viewport": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/jest-axe": "^3.5.9",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "babel-plugin-formatjs": "^10.5.10",
    "concurrently": "^8.2.2",
    "core-js": "^3.35.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "hydrogen-webpack-plugin": "*",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsconfig": "*",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  }
}
