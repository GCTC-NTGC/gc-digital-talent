    "@swc/core": "^1.4.5",
    "@types/react": "^18.2.64",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
