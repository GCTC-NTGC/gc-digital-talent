    "codegen": "graphql-codegen --config codegen.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-codegen/client-preset-swc-plugin": "^0.2.0",
    "@parcel/watcher": "^2.4.1",
    "concurrently": "^8.2.2",
    "graphql": "16.8.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "urql": "^4.0.6"
  }
}
